  "scripts": {
    "postinstall": "selenium-standalone install",
    "clean": "del ./dist/*",
    "tests": "run-p test eslint",
    "reports": "run-p test-report eslint-report coverage-summary",
    "test": "cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=test jest ",
    "test-report": "allure generate ./reports/allure-results/jest -o ./reports/jest",
    "eslint": "cross-env NODE_ENV=test eslint 'src/**/*.js' 'src/**/*.jsx' 'tasks/**/*.js' 'tests/**/*.js'",
    "eslint-json-summary": "npm run eslint -- -f json > ./reports/eslint.json",
    "eslint-report": "cross-env NODE_ENV=test eslint 'src/**/*.js' 'src/**/*.jsx' 'tasks/**/*.js' 'tests/**/*.js' -f node_modules/eslint-html-reporter/reporter.js -o reports/eslint.html",
    "coverage-summary": "cross-env NODE_ENV=test istanbul report --root ./ --include reports/coverage/coverage-final.json --dir ./reports json-summary",
    "serve": "cross-env NODE_CONFIG_DIR=./config/env webpack-dev-server",
    "serve-mock": "cross-env NODE_CONFIG_DIR=./config/env node ./tests/mocked/har-reader.js && cross-env NODE_CONFIG_DIR=./config/env node ./tests/mocked/dev-har-server.js",
    "storybook": "cross-env NODE_CONFIG_DIR=./config/env start-storybook --dont-track -p 9001",
    "build-production": "npm run clean && cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=production webpack --config webpack.config.production.js --optimize-minimize",
    "prep-translations": "cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=development gulp prepTranslations",
    "check-translations": "cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=development gulp checkTranslations",
    "e2e-local": "cross-env PATH=$PATH:node_modules/chromedriver/bin/ wdio ./config/webdriverio/wdio.local-with-chrome.conf.js",
    "e2e-local-firefox": "cross-env PATH=$PATH:node_modules/geckodriver/bin/ wdio ./config/webdriverio/wdio.local-with-firefox.conf.js",
    "e2e-local-safari": "wdio ./config/webdriverio/wdio.local-with-safari.conf.js",
    "e2e-local-with-mock": "del ./reports/allure-results/cucumber/* && del ./reports/allure-reports/cucumber && concurrently --kill-others --success first \"npm run serve-mock\" \"npm run e2e-local -- --baseUrl=http://localhost:8090\" && allure generate ./reports/allure-results/webdriverio -o ./reports/webdriverio"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "pre-commit": [
    "tests"
  ]
}
